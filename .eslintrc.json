{
    "env": {
        "browser": true,

        "es2021": true
    },
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": ["prettier", "react"],
    "rules": {
        "no-unused-vars": "warn",
        "react/jsx-uses-vars": "error",
        "react/react-in-jsx-scope": "off",
        "react/prop-types": "off",
        "react/display-name": "off",
        "no-undef": "warn",
        "react/no-children-prop": "off"
    }
}
